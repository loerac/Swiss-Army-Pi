{
  "type": "object",
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "minimum": 6.0,
      "maximum": 7.0
    },
    "region": {
      "$id": "#/properties/region",
      "type": "object",
      "required": [
        "country",
        "city"
      ],
      "properties": {
        "country": {
          "$id": "#/properties/region/properties/country",
          "type": "string"
        },
        "city": {
          "$id": "#/properties/region/properties/city",
          "type": "object",
          "required": [
            "name",
            "zipcode"
          ],
          "properties": {
            "name": {
              "$id": "#/properties/region/properties/city/properties/name",
              "type": "string"
            },
            "zipcode": {
              "$id": "#/properties/region/properties/city/properties/zipcode",
              "type": "integer",
              "minimum": 3.0,
              "maximum": 5.0
            }
          }
        }
      }
    },
    "coord": {
      "$id": "#/properties/coord",
      "type": "object",
      "required": [
        "lon",
        "lat"
      ],
      "properties": {
        "lon": {
          "$id": "#/properties/coord/properties/lon",
          "type": "double"
        },
        "lat": {
          "$id": "#/properties/coord/properties/lat",
          "type": "double"
        }
      }
    },
    "units": {
      "$id": "#/properties/units",
      "type": "integer"
    },
    "language": {
      "$id": "#/properties/language",
      "type": "string",
      "minLength": 2,
      "maxLength": 5
    },
    "key": {
      "$id": "#/properties/key",
      "type": "string"
    }
  },
  "required": [
    "key"
  ]
}
